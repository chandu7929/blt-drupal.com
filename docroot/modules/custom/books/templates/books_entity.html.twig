{#
/**
 * @file books_entity.html.twig
 * Default theme implementation to present Books data.
 *
 * This template is used when viewing Books pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_books_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('books_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
